<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="gridPane" hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" minWidth="50.0" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="50.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="200.0" />
        <ColumnConstraints hgrow="NEVER" minWidth="50.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="NEVER" />
    </rowConstraints>
    <children>
        <Button mnemonicParsing="false" text="|&lt;&lt;" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" text="&lt;&lt;" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" onAction="#handlePlay" text="&gt;" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" onAction="#handlePause" text="||" GridPane.columnIndex="4" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" onAction="#handleForward" text="&gt;&gt;" GridPane.columnIndex="5" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" text="&gt;&gt;|" GridPane.columnIndex="6" GridPane.rowIndex="1" />
        <TextField fx:id="openMediaFileTextField" editable="false" maxWidth="1E308" GridPane.columnIndex="8" GridPane.hgrow="ALWAYS"
            GridPane.rowIndex="1" />
        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#selectMediaFile" text="Open" GridPane.columnIndex="9"
            GridPane.hgrow="NEVER" GridPane.rowIndex="1" />
    </children>
</GridPane>
