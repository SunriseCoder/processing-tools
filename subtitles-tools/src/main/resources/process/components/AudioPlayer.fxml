<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="gridPane" hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" minWidth="50.0" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="NEVER" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="50.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="200.0" />
        <ColumnConstraints hgrow="NEVER" minWidth="40.0" />
        <ColumnConstraints hgrow="NEVER" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="NEVER" />
    </rowConstraints>
    <children>
        <Slider focusTraversable="false" fx:id="volumeSlider" max="1.0" orientation="VERTICAL" value="0.7" GridPane.columnIndex="12" />
        <Button focusTraversable="false" mnemonicParsing="false" text="|&lt;&lt;" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#handleSetSelectionStart" text="[" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#handleToSelectionStart" text="[&lt;&lt;" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#handlePlay" text="&gt;" GridPane.columnIndex="4" GridPane.rowIndex="1" />
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#handlePause" text="||" GridPane.columnIndex="5" GridPane.rowIndex="1" />
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#handleToSelectionEnd" text="&gt;&gt;]" GridPane.columnIndex="6" GridPane.rowIndex="1" />
        <Button focusTraversable="false" mnemonicParsing="false" onAction="#handleSetSelectionEnd" text="]" GridPane.columnIndex="7" GridPane.rowIndex="1" />
        <Button focusTraversable="false" mnemonicParsing="false" text="&gt;&gt;|" GridPane.columnIndex="8" GridPane.rowIndex="1" />
        <TextField fx:id="openMediaFileTextField" editable="false" focusTraversable="false" maxWidth="1E308" GridPane.columnIndex="10" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" />
        <Button focusTraversable="false" maxWidth="1E308" mnemonicParsing="false" onAction="#selectMediaFile" text="Open" GridPane.columnIndex="11" GridPane.columnSpan="2" GridPane.hgrow="NEVER" GridPane.rowIndex="1" />
    </children>
</GridPane>
