<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane minHeight="300.0" minWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <TreeTableView fx:id="treeTableView" maxHeight="1E308">
            <columns>
                <TreeTableColumn fx:id="columnPlace" prefWidth="75.0" text="Place" />
                <TreeTableColumn fx:id="columnLogin" prefWidth="75.0" text="Login" />
                <TreeTableColumn fx:id="columnPassword" prefWidth="75.0" sortable="false" text="Password" />
                <TreeTableColumn fx:id="columnComment" prefWidth="75.0" text="Comment" />
            </columns>
        </TreeTableView>
    </children>
</GridPane>
